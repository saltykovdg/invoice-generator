<?xml version="1.0" encoding="UTF-8"?>
<fontFamilies>
    <fontFamily name="Courier New">
        <normal>jasperreports/fonts/cour/cour.ttf</normal>
        <bold>jasperreports/fonts/cour/courbd.ttf</bold>
        <italic>jasperreports/fonts/cour/couri.ttf</italic>
        <boldItalic>jasperreports/fonts/cour/courbi.ttf</boldItalic>
        <pdfEncoding>Identity-H</pdfEncoding>
        <pdfEmbedded>true</pdfEmbedded>
        <exportFonts>
            <export key="net.sf.jasperreports.html">'Courier New', sans-serif</export>
            <export key="net.sf.jasperreports.xhtml">'Courier New', sans-serif</export>
        </exportFonts>
    </fontFamily>

    <!--
        Mapping Java logical fonts, for the sake of HTML export only.
    -->

    <fontFamily name="Courier New">
        <exportFonts>
            <export key="net.sf.jasperreports.html">'Courier New', sans-serif</export>
            <export key="net.sf.jasperreports.xhtml">'Courier New', sans-serif</export>
        </exportFonts>
    </fontFamily>
</fontFamilies>
